# test FSM
# DC26

$ver 100

finitestatemachine

$include "generic.fsm"

const GREET     "Hello World"
const INT_VAL   27
const PI        3.14

startstate IDLE

state IDLE
    signal T_5
        a say GREET
        s response PI
        s feedback TRUE
        ns IDLE
    signal T_20
        s drop_call
        exit
end